
# EX-G pro 유무선 하이스펙 트랙볼 마우스 M-XPT1MRBK
"xdotool key Prior" # wheel <-
  b:6
"xdotool key Next"  # wheel ->
  b:7

# most left/right-buttons
"xdotool key alt+Left"
  b:10
"xdotool key alt+Right"
  b:11

# the center btn
"xdotool key Next"
  b:12

# two up/down btns
#"xdotool key alt+Right"
"xdotool key Next" # wheel ->
  b:8
#"xdotool key alt+Left"
"xdotool key Prior" # wheel <-
  b:9


"xbindkeys_show"
  Mod4 + Shift + F1


#EOF
