

# EX-G pro 유무선 하이스펙 트랙볼 마우스 M-XPT1MRBK
#
#
#       L      WHL     R
#      (1)     (2)    (3)
#
# (10) (9)     ^4     (11)
#      (8)  (6)v5(7)
#
#              (12)
#

"xdotool key Prior"
  b:6
"xdotool key Next"
  b:7

"xdotool key Up"
  b:9
"xdotool key Down"
  b:8

"xdotool key Left"
  b:10
"xdotool key Right"
  b:11

"xdotool key space"
  b:12


"xbindkeys_show"
  Mod4 + Shift + F1


#EOF
