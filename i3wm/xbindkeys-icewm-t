"icesh -w focus moveby -100 0"
  Mod4 + Mod1 + Left
"icesh -w focus moveby 100 0"
  Mod4 + Mod1 + Right
"icesh -w focus moveby 0 -100"
  Mod4 + Mod1 + Up
"icesh -w focus moveby 0 100"
  Mod4 + Mod1 + Down

"icesh -w focus moveby -5 0"
  Shift + Mod4 + Mod1 + Left
"icesh -w focus moveby 5 0"
  Shift + Mod4 + Mod1 + Right
"icesh -w focus moveby 0 -5"
  Shift + Mod4 + Mod1 + Up
"icesh -w focus moveby 0 5"
  Shift + Mod4 + Mod1 + Down

"icesh -w focus sizeby -10% 0"
  Mod4 + Control + Left
"icesh -w focus sizeby +10% 0"
  Mod4 + Control + Right
"icesh -w focus sizeby 0 -10%"
  Mod4 + Control + Up
"icesh -w focus sizeby 0 +10%"
  Mod4 + Control + Down

"icesh -w focus sizeby -1% 0"
  Shift + Mod4 + Control + Left
"icesh -w focus sizeby 1% 0"
  Shift + Mod4 + Control + Right
"icesh -w focus sizeby 0 -1%"
  Shift + Mod4 + Control + Up
"icesh -w focus sizeby 0 +1%"
  Shift + Mod4 + Control + Down


