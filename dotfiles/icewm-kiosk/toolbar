# /usr/share/icons/gnome/32x32/apps

prog "rofi-menu" gnome-main-menu.png rofi -combi-modi drun,window,run -show combi
prog "scripts-menu" gtk-run.png scripts-rofi.pl -e -s


prog "mixer" volume-knob.png sh -c 'which pulsemixer && x-terminal-emulator -e pulsemixer || pavucontrol'

prog "fm4u" volume-knob.png fm4u.pl


